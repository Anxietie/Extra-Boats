{
	"schemaVersion": 1,
	"id": "anx",
	"version": "${version}",
	"name": "Extra Boats",
	"description": "This mod adds warped and crimson boat types to the game as completely new and separate boats.",
	"authors": [
		"Anxietie"
	],
	"contact": {
		"homepage": "https://www.curseforge.com/minecraft/mc-mods/extra-boats",
		"sources": "https://github.com/Anxietie/Extra-Boats.git",
		"issues": "https://github.com/Anxietie/Extra-Boats/issues"
	},
	"license": "GPLv3",
	"icon": "assets/anx/icon.png",
	"accessWidener" : "anx.accesswidener",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.mod.ExtraBoats"
		],
		"client": [
			"com.mod.ExtraBoatsClient"
		]
	},
	"mixins": [
		"anx.mixins.json",
		{
			"config": "anx.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	}
}